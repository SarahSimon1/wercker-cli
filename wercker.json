{
    "custom steps" : {
        "install test dependencies": [
            "pip install coverage",
            "pip install httpretty",
            "pip install mock",
            "pip install pep8"
        ],
        "pep8": "pep8 .",
        "unittest": [
            "coverage run  --timid --source=. -m unittest discover -v",
            "coverage report -m --omit=*/tests/*,setup*"
        ]
    },
    "deploy": [
        "./deploy.sh"
    ]
}
